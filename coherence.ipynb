import numpy as np
import matplotlib.pyplot as plt
from math import cosh

# --- 1. PARÂMETROS FINAIS DA LEI DE COERÊNCIA GEOMÉTRICA (PHI) ---

# Estes são os valores que geraram o boost de 4% com o pico em z=12,
# validando a Coerência Física e Geométrica.
alpha_final = 0.08      # Constante de Coerência (G_eff multiplier)
beta_final = 1.18       # Fator de Decaimento (Derivado da curvatura k)
gamma_final = 11.92     # Redshift do Pico (z_turn - Onde o JWST exige o boost)

# Parâmetros de Densidade (Flat Universe: Omega_total = 1)
Omega_m0 = 0.31         # Matéria (Bariônica + CDM)
Omega_r0 = 9.2e-5       # Radiação
Omega_Lambda_final = 1.0 - Omega_m0 - Omega_r0 

# Parâmetros LCDM (Para Comparação)
# O LCDM aqui é o modelo padrão de referência, sem o termo alpha
Omega_L_LCDM = Omega_Lambda_final 


# --- 2. FUNÇÃO DE EXPANSÃO (H(z)/H0) COERENTE VIA G_eff ---

def E_squared_coherent(z, alpha, beta, gamma, Omega_m0, Omega_r0, Omega_Lambda):
    """Calcula E^2(z) usando o Protocolo Phi na forma G_eff."""
    
    # Termo f(z): O sech^2 que define o perfil do vazamento geométrico
    ln_term = beta * np.log(1 + z) - beta * np.log(1 + gamma)
    f_z = 1 / (np.cosh(ln_term) ** 2)
    
    # O Coherence Term (1 + alpha*f(z)) atua como G_eff(z)
    Coherence_Term = 1 + alpha * f_z

    # Termos de Matéria e Radiação (MODIFICADOS pelo G_eff)
    term_matter_rad = (Omega_m0 * (1 + z)**3 + Omega_r0 * (1 + z)**4) * Coherence_Term

    # E^2 final: Termos modificados + Constante Lambda standard
    E2 = term_matter_rad + Omega_Lambda
    
    return E2

def E_coherent(z, *params):
    return np.sqrt(E_squared_coherent(z, *params))

def E_LCDM_pure(z):
    # E^2 para LCDM de referência (sem o termo alpha)
    return np.sqrt(Omega_m0 * (1 + z)**3 + Omega_r0 * (1 + z)**4 + Omega_L_LCDM)


# --- 3. GERAÇÃO E PLOTAGEM ---

z_range = np.linspace(0.0, 20.0, 500)
params_phi_coherent = (alpha_final, beta_final, gamma_final, Omega_m0, Omega_r0, Omega_Lambda_final)

E_phi_coherent = E_coherent(z_range, *params_phi_coherent)
E_lcdm_pure = E_LCDM_pure(z_range)

# Cálculo da Razão (O Boost)
ratio_boost_coherent = E_phi_coherent / E_lcdm_pure

# Plotagem da Coerência Geométrica
plt.figure(figsize=(10, 6))
plt.plot(z_range, ratio_boost_coherent, label='Razão $H_{\\phi} / H_{\\Lambda CDM}$ ($G_{eff}$ Coerente)', color='darkblue', linewidth=3)
plt.axhline(y=1.0, color='red', linestyle='--', label='Referência $\\Lambda$CDM (Razão = 1.0)')
plt.axvline(x=gamma_final, color='gray', linestyle=':', label=f'Pico Exigido ($z \\approx {gamma_final:.2f}$)')

plt.xlabel('Redshift (z)')
plt.ylabel('Razão de Expansão $H_{\\phi}(z) / H_{\\Lambda CDM}(z)$')
plt.title('Protocolo $\\phi$: Prova Visual da Coerência Geométrica')
plt.grid(True, which="both", ls="--", alpha=0.5)
plt.xlim(0, 20)
plt.ylim(1.0, 1.1)
plt.legend()
plt.show()
